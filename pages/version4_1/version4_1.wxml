  <view class="layout1" wx:if="{{login}}" style="margin-top: 943rpx;"  bindtap="showModal_num" data-type="{{3}}"></view>
  <view class="layout1" wx:if="{{login}}" style="margin-top: 1177rpx;"  bindtap="showModal_num" data-type="{{4}}"></view>
