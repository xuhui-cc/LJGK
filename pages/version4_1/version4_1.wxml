  <button class='pdf_wxbtn' wx:if="{{!login}}" open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber" data-type="{{7}}"></button>
  <!-- <button class='pdf_wxbtn' style="margin-top: 515rpx;" wx:if="{{!login}}" open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber" data-type="{{1}}"></button>
  <button class='pdf_wxbtn' style="margin-top: 725rpx;" wx:if="{{!login}}" open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber" data-type="{{2}}"></button>
  <button class='pdf_wxbtn' style="margin-top: 943rpx;" wx:if="{{!login}}" open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber" data-type="{{3}}"></button>
  <button class='pdf_wxbtn' style="margin-top: 1177rpx;" wx:if="{{!login}}" open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber" data-type="{{4}}"></button>
  <button class='pdf_wxbtn' style="margin-top: 1392rpx;" wx:if="{{!login}}" open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber" data-type="{{5}}"></button> -->
